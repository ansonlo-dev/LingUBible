# Font caching optimization
/fonts/optimized/*:
  Cache-Control: public, max-age=31536000, immutable
  Access-Control-Allow-Origin: *

# General font caching
/fonts/*:
  Cache-Control: public, max-age=2592000
  Access-Control-Allow-Origin: *

# CSS caching
*.css:
  Cache-Control: public, max-age=31536000

# JavaScript caching  
*.js:
  Cache-Control: public, max-age=31536000 